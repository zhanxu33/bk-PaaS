{
  "name": "lesscode",
  "version": "0.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib/client/dist",
    "clean-dll": "rm -rf lib/client/static/lib*",
    "build": "better-npm-run build",
    "local": "better-npm-run local",
    "online": "better-npm-run online",
    "dll": "better-npm-run dll",
    "dev": "better-npm-run dev",
    "stop": "FOREVER_ROOT=./ forever stop lesscode",
    "list": "FOREVER_ROOT=./ forever list",
    "logs": "FOREVER_ROOT=./ forever logs"
  },
  "betterScripts": {
    "dll": {
      "command": "npm run clean-dll && node lib/client/build/build-dll.js"
    },
    "build": {
      "command": "npm run clean && npm run dll && node lib/client/build/build.js",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": ""
      }
    },
    "dev": {
      "command": "npm run dll && nodemon --config nodemon.json",
      "env": {
        "NODE_ENV": "development",
        "APP_CODE": ""
      }
    },
    "local": {
      "command": "npm run build && FOREVER_ROOT=./ forever --minUptime 1000 --spinSleepTime 2000 start forever.json",
      "env": {
        "NODE_ENV": "local",
        "APP_CODE": ""
      }
    },
    "online": {
      "command": "npm run build && FOREVER_ROOT=./ forever --minUptime 1000 --spinSleepTime 2000 start forever.json",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": ""
      }
    }
  },
